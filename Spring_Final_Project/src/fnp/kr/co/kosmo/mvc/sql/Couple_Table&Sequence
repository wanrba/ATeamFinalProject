-- COUPLE Table Create SQL
CREATE TABLE COUPLE
(
    COUPLE_NUM    NUMBER          NULL, 
    FIRSTID       VARCHAR2(70)    NULL, 
    SECONDID      VARCHAR2(70)    NULL, 
    CONSTRAINT PK_COUPLE PRIMARY KEY (COUPLE_NUM)
)
/

CREATE SEQUENCE COUPLE_SEQ
START WITH 1
INCREMENT BY 1;
/


COMMENT ON TABLE COUPLE IS 'COUPLE'
/

COMMENT ON COLUMN COUPLE.COUPLE_NUM IS '커플 고유번호'
/

COMMENT ON COLUMN COUPLE.FIRSTID IS '등록자 아이디'
/

COMMENT ON COLUMN COUPLE.SECONDID IS '피등록자 아이디'
/