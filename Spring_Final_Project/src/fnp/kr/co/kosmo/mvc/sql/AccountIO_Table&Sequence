--통장입출테이블(ACCOUNTIO) - ACCOUNT IN OUT
CREATE TABLE ACCOUNTIO(
    ACCOUNT_INDEX NUMBER CONSTRAINT ACCOUNT_INDEX_NN NOT NULL,    			-- 통장 INDEX
    ACCOUNTIO_NUM NUMBER CONSTRAINT ACCOUNTIO_NUM_PK PRIMARY KEY,          -- 입 / 출 INDEX           
    DEPOSIT NUMBER,                                                        -- 입금
    WITHDRAW NUMBER,	                                                   -- 출금
    ACCOUNTIO_USERNUM NUMBER CONSTRAINT ACCOUNTIO_USERNUM_NN NOT NULL,      --유저 INDEX
    ACCOUNTIO_DATE DATE DEFAULT SYSDATE, 		                                       --사용일
    CONSTRAINT ACCOUNTIO_USERNUM_FK FOREIGN KEY(ACCOUNTIO_USERNUM) REFERENCES MEMBER(USER_NUM),
    CONSTRAINT ACCOUNTIO_account_index_FK FOREIGN KEY(ACCOUNT_INDEX) REFERENCES account(account_index)
);



CREATE SEQUENCE ACCOUNTIO_SEQ
INCREMENT BY 1
START WITH 1;